{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Posts",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "board": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::board.board"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "featured": {
      "type": "boolean"
    },
    "attachments": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "contactPhone": {
      "type": "string"
    },
    "wechat": {
      "type": "string"
    },
    "city": {
      "type": "string",
      "required": false
    },
    "category": {
      "type": "string",
      "required": false
    },
    "contactEmail": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "post"
    },
    "refreshedAt": {
      "type": "datetime"
    },
    "address": {
      "type": "string"
    },
    "websiteUrl": {
      "type": "string"
    },
    "content": {
      "type": "customField",
      "options": {
        "preset": "defaultHtml"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
